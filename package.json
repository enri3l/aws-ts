{
  "name": "aws-ts-cli",
  "version": "0.1.0",
  "description": "TypeScript-based CLI for AWS operations with modular architecture and comprehensive testing",
  "keywords": [
    "aws",
    "cli",
    "typescript",
    "oclif",
    "dynamo",
    "sdk"
  ],
  "homepage": "https://github.com/enri3l/aws-ts#readme",
  "bugs": {
    "url": "https://github.com/enri3l/aws-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enri3l/aws-ts.git"
  },
  "main": "./dist/index.js",
  "bin": {
    "aws-ts": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx watch src/index.ts",
    "dev:debug": "node --inspect-brk --loader tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:changed": "vitest run --changed",
    "test:unit": "vitest run --project unit",
    "test:unit:changed": "vitest run --changed --project unit",
    "test:integration": "vitest run --project integration",
    "test:integration:changed": "vitest --changed --project integration",
    "test:e2e": "vitest run --project e2e",
    "test:e2e:changed": "vitest run --changed --project e2e",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . && markdownlint-cli2",
    "lint:fix": "eslint . --fix && markdownlint-cli2 --fix",
    "lint:changed": "eslint --no-error-on-unmatched-pattern $(git diff --name-only HEAD~1 HEAD -- '*.ts' '*.tsx' '*.js' '*.jsx' | tr '\\n' ' ') && markdownlint-cli2 $(git diff --name-only HEAD~1 HEAD -- '*.md' | tr '\\n' ' ') || true",
    "format": "prettier --log-level warn . --check",
    "validate": "pnpm typecheck && pnpm format:fix && pnpm lint:fix && pnpm test:run",
    "check:changed": "pnpm test:changed && pnpm lint:changed",
    "format:fix": "prettier --log-level warn . --write",
    "prepare": "lefthook install",
    "prebuild": "rimraf dist",
    "clean": "rimraf dist coverage .nyc_output",
    "docs:build": "pnpm docs:api && vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "docs:api": "typedoc --out docs/api src/index.ts",
    "docs:validate": "pnpm build && node dist/index.js --help > /dev/null"
  },
  "author": "Enrico Lionello",
  "license": "Apache-2.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.15.0",
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "^9.34.0",
    "@oclif/test": "4.1.14",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "3.2.4",
    "aws-sdk-client-mock": "4.1.0",
    "aws-sdk-client-mock-vitest": "6.2.0",
    "eslint": "^9.34.0",
    "eslint-plugin-jsdoc": "55.1.2",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-tsdoc": "0.4.0",
    "eslint-plugin-unicorn": "61.0.2",
    "lefthook": "^1.12.3",
    "markdownlint-cli2": "^0.18.1",
    "memfs": "4.42.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "rimraf": "6.0.1",
    "testcontainers": "11.5.1",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitepress": "^1.6.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.883.0",
    "@aws-sdk/client-sts": "3.883.0",
    "@aws-sdk/credential-provider-sso": "3.883.0",
    "@aws-sdk/credential-providers": "3.883.0",
    "@aws-sdk/lib-dynamodb": "3.883.0",
    "@aws-sdk/util-dynamodb": "3.883.0",
    "@oclif/core": "4.5.3",
    "@oclif/plugin-help": "6.2.33",
    "@oclif/plugin-plugins": "5.4.47",
    "ora": "9.0.0",
    "zod": "4.1.5"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "oclif": {
    "bin": "aws-ts",
    "dirname": "aws-ts",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": ":",
    "topics": {
      "auth": {
        "description": "AWS authentication and profile management commands"
      }
    }
  }
}
