{
  "name": "@monte3l/aws-ts",
  "version": "0.4.0",
  "description": "TypeScript-based CLI for AWS operations with modular architecture",
  "keywords": [
    "aws",
    "cli",
    "typescript",
    "oclif",
    "dynamo",
    "sdk"
  ],
  "homepage": "https://github.com/monte3l/aws-ts#readme",
  "bugs": {
    "url": "https://github.com/monte3l/aws-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monte3l/aws-ts.git"
  },
  "main": "./dist/index.js",
  "bin": {
    "aws-ts": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx watch src/index.ts",
    "dev:debug": "node --inspect-brk --loader tsx src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . && markdownlint-cli2",
    "lint:fix": "eslint . --fix && markdownlint-cli2 --fix",
    "lint:changed": "eslint --no-error-on-unmatched-pattern $(git diff --name-only HEAD~1 HEAD -- '*.ts' '*.tsx' '*.js' '*.jsx' | tr '\\n' ' ') && markdownlint-cli2 $(git diff --name-only HEAD~1 HEAD -- '*.md' | tr '\\n' ' ') || true",
    "format": "prettier --log-level warn . --check",
    "validate": "pnpm typecheck && pnpm format:fix && pnpm lint:fix",
    "format:fix": "prettier --log-level warn . --write",
    "prepare": "lefthook install",
    "prebuild": "rimraf dist",
    "clean": "rimraf dist",
    "docs:build": "pnpm docs:api && vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "docs:api": "typedoc --out docs/api src/index.ts",
    "docs:validate": "pnpm build && node dist/index.js --help > /dev/null",
    "publish:jsr": "pnpm dlx jsr publish",
    "publish:jsr:dry-run": "pnpm dlx jsr publish --dry-run"
  },
  "author": "Enrico Lionello",
  "license": "Apache-2.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.15.0",
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "eslint-plugin-jsdoc": "60.1.1",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-tsdoc": "0.4.0",
    "eslint-plugin-unicorn": "61.0.2",
    "lefthook": "^1.13.2",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "rimraf": "6.0.1",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitepress": "^1.6.4"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "3.896.0",
    "@aws-sdk/client-apigatewayv2": "3.896.0",
    "@aws-sdk/client-cloudwatch-logs": "3.896.0",
    "@aws-sdk/client-dynamodb": "3.895.0",
    "@aws-sdk/client-ecs": "3.895.0",
    "@aws-sdk/client-eventbridge": "3.895.0",
    "@aws-sdk/client-lambda": "3.895.0",
    "@aws-sdk/client-s3": "3.895.0",
    "@aws-sdk/client-sts": "3.896.0",
    "@aws-sdk/credential-provider-sso": "3.895.0",
    "@aws-sdk/credential-providers": "3.895.0",
    "@aws-sdk/lib-dynamodb": "3.895.0",
    "@aws-sdk/util-dynamodb": "3.895.0",
    "@oclif/core": "4.5.4",
    "@oclif/plugin-help": "6.2.33",
    "@oclif/plugin-plugins": "5.4.47",
    "@types/inquirer": "9.0.9",
    "enquirer": "2.4.1",
    "execa": "9.6.0",
    "inquirer": "12.9.6",
    "listr2": "9.0.4",
    "ora": "9.0.0",
    "zod": "4.1.11"
  },
  "oclif": {
    "bin": "aws-ts",
    "dirname": "aws-ts",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": ":",
    "topics": {
      "apigw": {
        "description": "API Gateway discovery and configuration commands"
      },
      "auth": {
        "description": "AWS authentication and profile management commands"
      },
      "cloudwatch": {
        "description": "CloudWatch monitoring and logging commands",
        "subtopics": {
          "logs": {
            "description": "CloudWatch Logs management and streaming commands"
          }
        }
      },
      "dynamodb": {
        "description": "DynamoDB table operations and data management commands"
      },
      "ecs": {
        "description": "Amazon ECS container orchestration and service management commands"
      },
      "eventbridge": {
        "description": "Amazon EventBridge rule and target management commands"
      },
      "lambda": {
        "description": "AWS Lambda function management and invocation commands"
      }
    }
  }
}
